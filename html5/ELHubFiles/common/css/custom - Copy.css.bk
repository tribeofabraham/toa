/*--- Media Button */
.med .btn {
    border-radius:50%;
    background-color:var(--ghost20);
    outline: none;
    border:solid .25em rgba(0,0,0,0);
    cursor:pointer;
    transition: all 1s 0s;
}


.med .btn:active {
    border:solid .25em var(--ghost80);
}
.med .btn:focus {
    border:solid .25em var(--ghost60);
    transition: all 0s 0s;
}
.med .btn:hover {
    border:solid .25em var(--ghost80);
    transition: all 0s 0s;
}
.med .btn .btnIcBack {
    position: absolute;
    left:0;
    right:0;
    bottom:0;
    top:0;
    opacity:.4;
    transition: all 1s 0s;
}
.med .btn:hover .btnIcBack {
    transition: all 0s 0s;
    opacity:.8;
}
.med .btn:focus .btnIcBack {
    opacity:1;
    transition: all 0s 0s;
}
.med.dark .btn {
    background-color:var(--ghost40);
}
.med.light .btn {
    background-color:var(--ghostBlack40);
}
.med.dark .btn:active {
    border:solid .25em var(--ghost80);
}
.med.light .btn:active {
    border:solid .25em var(--ghostBlack80);
}

.med.dark .btn:focus {
    border:solid .25em var(--ghost60);
}

.med.light .btn:focus {
    border:solid .25em var(--ghostBlack60);
}

.med.dark .btn:hover {
    border:solid .25em var(--ghost80);
    background-color: var(--ghost80);
}
.med.light .btn:hover {
    border:solid .25em var(--ghostBlack80);
    background-color: var(--ghostBlack80);
}



