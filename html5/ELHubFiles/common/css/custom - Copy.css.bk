    box-shadow: 0em 0em .5em rgba(0,0,0,.1);
}
.mph #wrapper .logo{
    border-radius:0;
}

.mph #wrapper .heading{
    text-align: left;
    font-size:2em;
    margin-left:1.75em;
    color:var(--primary);
}
.mph #wrapper .description{
    text-align: left;
    font-size:.5em;
    color:var(--ghostBlack80);
    padding:1em;
    font-family: "BodyText";
    line-height: 1.2em;
}
.mph #wrapper .grp .heading{
    text-align: center;
    font-size:.8em;
    color:var(--textWhite);
    margin-left:0em;
    text-align: left;
    padding:.25em;
    background-color:var(--ghostBlack40);
    line-height: 1.0em;
}

body.mph #wrapper .grp{
    background-size:auto 9.5em;
    background-repeat: no-repeat;
    background-position:center -.5em;
    border-radius:.4rem;
    margin:0;
    padding:0;
    border:none;
 
    
}

body.mph #wrapper .grp.grp1{
    background-image: url(../../assets/img/imgCovers/Playground.png);
}
body.mph #wrapper .grp.grp2{
 background-image: url(../../assets/img/imgCovers/ELBestPractices.png);
}
body.mph #wrapper .grp.grp3{
    background-image: url(../../assets/img/imgCovers/RGRespanol.png);
}
body.mph #wrapper .grp.grp4{
     background-image: url(../../assets/img/imgCovers/Infercabulary.jpg);
}

body.admin #wrapper {
    background-color:var(--textBlack);
}
body.admin #wrapper .sld{
    background-color:var(--control5);
}


/* Nick Admin Stuff for you here */
body.admin #wrapper .primary  {
    background-color:var(--primary);
}
body.admin #wrapper .secondary  {
    background-color:var(--secondary);
}
body.admin #wrapper .tertiary  {
    background-color:var(--tertiary);
}

body.admin #wrapper .grp{
    background-size:auto 9.5em;
    background-repeat: no-repeat;
    background-position:center -.5em;
    border-radius:.4rem;
    margin:0;
    padding:0;
    border:none;  
}

.admin #wrapper .btn, .admin #wrapper .lnk {
    background-color:var(--control1);
    color:var(--ghostWhite80);
    font-size:.5em;
    border-radius:.2em;
    padding:1em;
    height:auto;
    font-family: "DisplayText";
    color:var(--textWhite);
    cursor:pointer;
    text-decoration: none;
    vertical-align: middle;
    text-align: center;
}

.admin #wrapper .btn.primary, .admin #wrapper .lnk.primary {
    background-color:var(--primary);
}

.admin #wrapper .btn.secondary, .admin #wrapper .lnk.secondary {
    background-color:var(--primary);
}
body.admin #wrapper .Logo{
    border-radius:0;
}

body.admin #wrapper .heading{
    color:var(--ghost80);
    text-align: left;
    font-size: 2em;
    padding: .5em;
}
body.admin #wrapper .grp .heading{
    color:var(--ghost80);
    background-color:var(--ghostBlack60);
    font-size:.75em;
}

.admin #wrapper .description{
    text-align: left;
    font-size:.5em;
    color:var(--ghostBlack80);
    padding:1em;
    font-family: "BodyText";
    line-height: 1.2em;
}


body.admin #wrapper .lst {
    position:absolute;
    padding:.25em;
    text-align: left;
    border-radius:.5em;
    overflow: visible;
    background-color:var(--ghostBlack20);
    
}


body.admin #wrapper .lst .itm  {
    font-family: "BoldText";
    color:var(--ghost80);
    border-radius:.5em;
    padding:.5em;
    cursor:pointer;
    margin:.25em;
    overflow: visible;
    position:relative;
}

body.admin #wrapper .lst .itm .toggleChildTrigger  {
    position:absolute;
    top:-.25em;
    bottom:-.25em;
    left:-.25em;
    right:-.25em;
    border-radius:.5em;
    cursor:pointer;
    border:solid .25em var(--ghost20);
}
body.admin #wrapper .lst .itm .toggleChildTrigger:focus  {
    border:solid .25em var(--ghost80);
}

body.admin #wrapper .lst .itm a {
    text-decoration: none;
    color:var(--ghost80);
    cursor:pointer;
}
body.admin #wrapper .lst .itm:hover  {
    color:var(--ghostBlack80);
}
body.admin #wrapper .lst  .lst  {
   display:none;
}
body.admin #wrapper .lst .itm .lst .itm  {
   margin:0;
}

body.admin #wrapper .lst .itm .lst .itm:hover  {
    background-color:var(--control5);
}

body.admin #wrapper .lst .itm .lst {
    background:var(--ghostBlack80);;
}

body.admin #wrapper .lst .itm .lst .itm  {
    font-size:.8em;
    background:none;
}



body.admin #wrapper .lst .itm .lst li.itm {
    display: block;
    
}
